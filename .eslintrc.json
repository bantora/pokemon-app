{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "quotes": [2, "single"],
        "space-after-keywords": ["error", "never"],
        "indent": ["error", 2],
        "no-import-assign": "error",
        "no-unused-vars": "warn",
        "no-multiple-empty-lines": ["warn", { "max": 1, "maxEOF": 1 , "maxBOF": 0}],
        "unused-imports/no-unused-imports": "error",
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true,
              "endOfLine": "auto"
            },
            { "usePrettierrc": true }
          ]
    }
}
